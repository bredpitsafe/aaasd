<dashboard>
  <version>1.0.0</version>
  <name>Chart settings xml</name>
  <grid>
    <name>80x45</name>
    <cols_count>80</cols_count>
    <rows_count>45</rows_count>
    <margin>8</margin>
    <panel>
      <rel_width>0.2</rel_width>
      <rel_height>0.2</rel_height>
      <rel_min_width>0.2</rel_min_width>
      <rel_min_height>0.2</rel_min_height>
    </panel>
  </grid>
  <panels>
    <panel>
      <type>Charts</type>
      <settings>
        <url>ws://localhost</url>
        <label>Test chart</label>
        <legends>true</legends>
        <fixed_min_y>1e-10</fixed_min_y>
        <fixed_max_y>1e10</fixed_max_y>
        <min_y>1e-9</min_y>
        <max_y>1e9</max_y>
        <fixed_min_y_right>1e-6</fixed_min_y_right>
        <fixed_max_y_right>1e6</fixed_max_y_right>
        <min_y_right>1e-5</min_y_right>
        <max_y_right>1e5</max_y_right>
        <display_zero_left>true</display_zero_left>
        <display_zero_right>false</display_zero_right>
        <min_zoom>1e-13</min_zoom>
        <max_zoom>1e13</max_zoom>
        <follow_mode>permament</follow_mode>
        <closest_points>true</closest_points>
        <time_zone>0</time_zone>
        <server_time_unit>nanosecond</server_time_unit>
        <server_time_increment>1609459200000000000</server_time_increment>
        <zoom_step_multiplier>3</zoom_step_multiplier>
        <do-not-initialize-charts>true</do-not-initialize-charts>
      </settings>
      <layout>
        <rel_width>0.3333333333333333</rel_width>
        <rel_height>1</rel_height>
        <rel_min_width>0.16666666666666666</rel_min_width>
        <rel_min_height>0.16666666666666666</rel_min_height>
        <rel_x>0</rel_x>
        <rel_y>0</rel_y>
      </layout>
      <charts>
        <chart>
          <id>indicators{name='BTC-ETH|UpbitSpot.l1.ask'}</id>
          <query>indicators{name='BTC-ETH|UpbitSpot.l1.ask'}</query>
          <label>indicators{name='BTC-ETH|UpbitSpot.l1.ask'}</label>
          <label_format>%s: %.4g</label_format>
          <type>points</type>
          <point_size>4</point_size>
          <color>blue</color>
          <opacity>1</opacity>
          <y_axis>left</y_axis>
          <width>4</width>
        </chart>
        <chart>
          <id>indicators{name='BTC-ETH|UpbitSpot.l2.ask'}</id>
          <url>ws://somehost</url>
          <query>indicators{name='BTC-ETH|UpbitSpot.l2.ask'}</query>
          <label>indicators{name='BTC-ETH|UpbitSpot.l2.ask'}</label>
          <label_format>%s: %.4g</label_format>
          <type>points</type>
          <point_size>4</point_size>
          <color>blue</color>
          <opacity>1</opacity>
          <y_axis>left</y_axis>
          <width>4</width>
        </chart>
      </charts>
    </panel>
  </panels>
</dashboard>
