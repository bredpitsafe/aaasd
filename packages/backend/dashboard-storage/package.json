{
  "name": "@backend/dashboard-storage",
  "version": "0.0.2",
  "description": "Authorized dashboard storage",
  "type": "module",
  "author": "@frontend",
  "scripts": {
    "start": "node --loader ts-node/esm ./src/index.ts",
    "start_with_coverage": "c8 --clean -r text -r html -r cobertura npm start",
    "dev": "nodemon ./src/index.ts",
    "build": "webpack",
    "types": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "start-server-and-test start http://localhost:8111 test_api",
    "test_api": "vitest --run --poolOptions.threads.singleThread",
    "jsonschema2ts": "node --loader ts-node/esm ./bin/jsonschema2ts.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@backend/grpc": "^1.0.0",
    "@backend/keycloak": "^0.0.1",
    "@backend/utils": "^1.0.0",
    "@common/rx": "^1.0.0",
    "@common/types": "^1.0.0",
    "@common/utils": "^1.0.0",
    "@grpc/grpc-js": "^1.9.13",
    "@grpc-schemas/dashboard_storage-api-sdk": "1.240729.0",
    "@xtuc/long": "^4.2.2",
    "bottleneck": "^2.19.5",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "node-fetch": "^3.3.1",
    "pg": "^8.11.3",
    "pg-format": "^1.0.4",
    "prom-client": "^14.2.0",
    "toml": "^3.0.0",
    "ts-node": "^10.9.2",
    "winston": "^3.8.2",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@backend/oauth-mock-server": "^1.0.0",
    "@types/config": "^3.3.3",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/pg": "^8.10.7",
    "@types/pg-format": "^1.0.4",
    "@types/ws": "^8.5.10",
    "c8": "^8.0.0",
    "json-schema-to-typescript": "^13.0.1",
    "nodemon": "^2.0.22",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
